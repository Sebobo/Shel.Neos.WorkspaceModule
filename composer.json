{
  "name": "shel/neos-workspace-module",
  "description": "An alternative workspace module with additional features for Neos CMS",
  "type": "neos-plugin",
  "license": "MIT",
  "keywords": [
    "flow",
    "neoscms",
    "workspaces"
  ],
  "require": {
    "php": ">=7.4",
    "neos/neos": "^5.3 || ^7.0 || ^8.0 || dev-master"
  },
  "autoload": {
    "psr-4": {
      "Shel\\Neos\\WorkspaceModule\\": "Classes"
    }
  },
  "extra": {
    "neos": {
      "package-key": "Shel.Neos.WorkspaceModule"
    }
  },
  "archive": {
    "exclude": [
      "/.yarn",
      "/.github"
    ]
  }
}

