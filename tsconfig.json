{
  "compilerOptions": {
    "skipLibCheck": true,
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "typeRoots": ["./Resources/Private/JavaScript/typings"]
  },
  "exclude": [
    "node_modules"
  ],
  "references": [
    {
      "path": "./Resources/Private/JavaScript"
    },
    {
      "path": "./Tests/dev-server"
    }
  ]
}
